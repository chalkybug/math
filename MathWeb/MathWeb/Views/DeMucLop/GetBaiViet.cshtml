@model IEnumerable<MathWeb.Models.Data.BaiViet>

@{
    ViewBag.Title = "GetBaiViet";
    Layout = "~/Views/Layout/_Layout.cshtml";
}


<div class="container">
    <div class="row">
        <div class="col-md-2">

        </div>
        <div class="col-md-10">
            <h3>@ViewBag.TenDeMuc</h3>
            @foreach (var item in Model)
            {
                <ul>
                    <li>
                        <a href="/BaiViet/GetBaiViet/@item.ID">@Html.DisplayFor(modelItem => item.TenBaiViet)</a>
                        
                    </li>
                    

                </ul>
            }
        </div>

    </div>
</div>
